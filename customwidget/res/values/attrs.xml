<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- checkbox -->
    <declare-styleable name="Checkbox">
        <attr name="checkboxLabel" format="string"></attr>
        <attr name="checkboxActive" format="boolean"></attr>
        <attr name="checkboxActiveImg" format="reference"></attr>
        <attr name="checkboxInActiveImg" format="reference"></attr>
    </declare-styleable>

    <!-- tabselector -->
    <declare-styleable name="TabItem">
        <attr name="textTabItem" format="string"></attr>
    </declare-styleable>
    
    <!-- BoundedLinearLayout -->

    <declare-styleable name="BoundedLinearLayout">
        <attr name="maxheight" format="dimension"></attr>
    </declare-styleable>
	<declare-styleable name="ExpandableTextView">
	    <attr name = "trimLength" format="integer"></attr>
	    
	</declare-styleable>
</resources>