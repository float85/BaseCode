<?xml version="1.0" encoding="utf-8"?>
<resources>
    <dimen name="abc_config_prefDialogWidth">440dp</dimen>
    <item name="abc_dialog_fixed_height_major" type="dimen">60%</item>
    <item name="abc_dialog_fixed_height_minor" type="dimen">90%</item>
    <item name="abc_dialog_fixed_width_major" type="dimen">60%</item>
    <item name="abc_dialog_fixed_width_minor" type="dimen">90%</item>
    <item name="abc_dialog_min_width_major" type="dimen">55%</item>
    <item name="abc_dialog_min_width_minor" type="dimen">80%</item>
    <dimen name="alertdialog_content_height">80dp</dimen>
    <dimen name="alertdialog_title_height">40dp</dimen>
    <dimen name="alertdialog_width">300dp</dimen>
    <dimen name="checkboximage_height">20dp</dimen>
    <dimen name="checkboximage_width">20dp</dimen>
    <dimen name="checkboxlabel_textsize">14sp</dimen>
    <dimen name="expand_listview_item_button_height">25sp</dimen>
    <dimen name="expand_listview_item_height">35dp</dimen>
    <dimen name="padding_icon_menu">10dp</dimen>
    <dimen name="padding_slidemenu_tablet">20dp</dimen>
    <dimen name="padding_slidemenu_tablet_logout">17dp</dimen>
    <dimen name="slidemenu_item_textsize">17sp</dimen>
    <dimen name="slidemenuitem_height">40sp</dimen>
    <dimen name="slidemenutitle_textsize">14sp</dimen>
    <dimen name="tabitem_textsize">15sp</dimen>
    <dimen name="tabselector_tabitem_height">35dp</dimen>
    <dimen name="width_slidemenu_tablet">67dp</dimen>
    <style name="ActionBar">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">@dimen/header_height</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:background">@drawable/actionbar_background</item>
    </style>
    <style name="AppBaseTheme" parent="@android:style/Theme.Holo.NoActionBar.Fullscreen"/>
    <style name="Base.Theme.AppCompat.DialogWhenLarge" parent="Base.Theme.AppCompat.Dialog.FixedSize"/>
    <style name="Base.Theme.AppCompat.Light.DialogWhenLarge" parent="Base.Theme.AppCompat.Light.Dialog.FixedSize"/>
    <style name="Button_Background1">
        <item name="android:textSize">@dimen/m_button_textsize</item>
        <item name="android:textColor">@color/button_text_color</item>
        <item name="android:background">@drawable/t_backgroundbutton_red</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/m_button_height</item>
    </style>
    <style name="Label_Background3">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@color/label_color</item>
        <item name="android:gravity">center_vertical|left</item>
        <item name="android:layout_width">150dp</item>
        <item name="android:layout_height">@dimen/textline_height</item>
    </style>
    <style name="MyTheme.ActionBarStyle" parent="ActionBar">
        <item name="android:background">#444444</item>
        <item name="android:titleTextStyle">@style/TextAppearance</item>
    </style>
    <style name="Spinner">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/m_spinner_height</item>
        <item name="android:layout_marginTop">4dp</item>
        <item name="android:background">@color/transparent</item>
    </style>
    <style name="TabSelector">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">35dp</item>
        <item name="android:layout_alignParentBottom">true</item>
        <item name="android:layout_marginBottom">2dp</item>
        <item name="android:background">@drawable/selector_menu_tab_selector</item>
        <item name="android:padding">5dp</item>
        <item name="android:textColor">@color/text_color</item>
    </style>
    <style name="TextAppearance" parent="@android:style/TextAppearance">
        <item name="android:textSize">15.0sp</item>
        <item name="android:textColor">#99ffffff</item>
        <item name="android:textStyle">normal</item>
    </style>
    <style name="Theme.AppCompat.Light.NoActionBar.FullScreen" parent="Base.Theme.AppCompat.Light">
        <item name="android:windowNoTitle">true</item>
        <item name="windowActionBar">false</item>
        <item name="android:typeface">sans</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>
    <style name="Theme.Base.AppCompat.Dialog.Light.FixedSize" parent="Base.Theme.AppCompat.Dialog.FixedSize"/>
    <style name="alertdialog_title_style" parent="android:Widget.TextView">
        <item name="android:background">@color/alertdialog_background_color</item>
        <item name="android:textColor">@color/text_color</item>
        <item name="android:textSize">@dimen/actionbar_title_size</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="cusAlertDialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowTitleStyle">@style/alertdialog_title_style</item>
        <item name="android:windowBackground">@color/alertdialog_background_color</item>
    </style>
    <style name="cusDialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowTitleStyle">@style/dialog_title_style</item>
    </style>
    <style name="dialog_title_style" parent="android:Widget.TextView">
        <item name="android:background">@drawable/header_dialogfragment</item>
        <item name="android:textColor">@color/actionbar_title_color</item>
        <item name="android:textSize">@dimen/actionbar_title_size</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="imageview_loading">
        <item name="android:visibility">gone</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">30dp</item>
        <item name="android:layout_marginRight">5dp</item>
        <item name="android:src">@drawable/ic_loading</item>
        <item name="android:scaleType">center</item>
    </style>
    <style name="label_style1">
        <item name="android:textColor">#ff000000</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingTop">3dp</item>
        <item name="android:paddingBottom">3dp</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_marginLeft">5dp</item>
    </style>
</resources>