<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:gravity="center_vertical"
        android:orientation="horizontal" >

        <Button
            android:id="@+id/btn_solenhgtc_item_cancel"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_weight="1"
            android:background="@color/transparent"
            android:text="@string/Cancel"
            android:textColor="@color/red_brown_text"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/text_solenhgtc_item_CustodyCd"
            style="@style/Content_Style"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="2"
            android:gravity="center"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/text_solenhgtc_item_afacctno"
            style="@style/Content_Style"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="2"
            android:gravity="center"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/text_solenhgtc_item_Side"
            style="@style/Content_Style"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:textAllCaps="true"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/text_solenhgtc_item_MaCK"
            style="@style/Content_Style"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/text_solenhgtc_item_SoLuong"
            style="@style/Content_Style"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1.5"
            android:gravity="center"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/text_solenhgtc_item_Gia"
            style="@style/Content_Style"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/text_solenhgtc_item_matchedqtty"
            style="@style/Content_Style"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1.5"
            android:gravity="center"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/text_solenhgtc_item_remainqtty"
            style="@style/Content_Style"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1.5"
            android:gravity="center"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/text_solenhgtc_item_fromDate"
            style="@style/Content_Style"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="2"
            android:gravity="center"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/text_solenhgtc_item_toDate"
            style="@style/Content_Style"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="2"
            android:gravity="center"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/text_solenhgtc_item_TrangThai"
            style="@style/Content_Style"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="2"
            android:gravity="center"
            android:textSize="12sp" />
    </LinearLayout>

</LinearLayout>