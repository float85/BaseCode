<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res/com.fss.tcbs.mobiletrading"
    android:id="@+id/datlenh"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/bg_dialogplaceorder"
    android:focusable="true"
    android:focusableInTouchMode="true"
    android:orientation="vertical" >

    <ScrollView
        android:id="@+id/scrollview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:fillViewport="true"
        android:scrollbars="none" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <LinearLayout
                android:id="@+id/lay_buysell"
                android:layout_width="@dimen/width_buy_sell"
                android:layout_height="35dp"
                android:layout_gravity="center_horizontal"
                android:layout_marginBottom="5dp"
                android:layout_marginTop="5dp"
                android:orientation="horizontal" >

                <Button
                    android:id="@+id/btn_DatLenh_Mua"
                    android:layout_width="0dp"
                    android:layout_height="fill_parent"
                    android:layout_weight="5.0"
                    android:background="@drawable/background_buy_placeorder"
                    android:text="@string/Mua"
                    android:textAllCaps="true"
                    android:textColor="@drawable/selector_textcolor_white_black"
                    android:textSize="20.0sp" />

                <Button
                    android:id="@+id/btn_DatLenh_Ban"
                    android:layout_width="0dp"
                    android:layout_height="fill_parent"
                    android:layout_weight="5.0"
                    android:layout_marginLeft="15dp"
                    android:background="@drawable/background_sell_placeorder"
                    android:text="@string/Ban"
                    android:textAllCaps="true"
                    android:textColor="@drawable/selector_textcolor_white_black"
                    android:textSize="20.0sp" />
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/header_separation_color" />

            <LinearLayout
                android:id="@+id/container_placeorder_stockindex"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/transparent"
                android:orientation="vertical" >
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/header_separation_color" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/bg_dialogplaceorderdown"
                android:orientation="horizontal"
                android:paddingLeft="@dimen/m_paddingLeft"
                android:paddingRight="@dimen/m_paddingRight" >

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="5"
                    android:orientation="vertical" >

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" >

                        <ImageView
                            android:id="@+id/img_placeorder_account"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_centerVertical="true"
                            android:scaleType="fitCenter"
                            android:src="@drawable/ic_account" />

                        <TextView
                            android:id="@+id/autoEdt_DatLenh_Afacctno"
                            style="@style/Content_Style"
                            android:layout_width="match_parent"
                            android:layout_height="45dp"
                            android:layout_marginLeft="10dp"
                            android:layout_toRightOf="@+id/img_placeorder_account"
                            android:hint="@string/TieuKhoan"
                            android:singleLine="true"
                            android:textSize="16sp" />

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="0.5dp"
                            android:layout_alignLeft="@+id/img_placeorder_account"
                            android:layout_alignRight="@+id/autoEdt_DatLenh_Afacctno"
                            android:layout_below="@+id/autoEdt_DatLenh_Afacctno"
                            android:background="@color/lc_separation_color" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" >

                        <ImageView
                            android:id="@+id/img_placeorder_symbol"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_centerVertical="true"
                            android:scaleType="fitCenter"
                            android:src="@drawable/ic_datlenh_mack" />

                        <EditText
                            android:id="@+id/autoEdt_DatLenh_Symbol"
                            style="@style/EditText_Style"
                            android:layout_width="match_parent"
                            android:layout_height="45dp"
                            android:layout_marginLeft="10dp"
                            android:layout_toRightOf="@+id/img_placeorder_symbol"
                            android:hint="@string/MaChungKhoan"
                            android:inputType="textCapCharacters"
                            android:singleLine="true"
                            android:textColorHint="@color/edittext_hint_color"
                            android:textCursorDrawable="@drawable/shape_textcursoredittext" />

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="0.5dp"
                            android:layout_alignLeft="@+id/img_placeorder_symbol"
                            android:layout_alignRight="@+id/autoEdt_DatLenh_Symbol"
                            android:layout_below="@+id/autoEdt_DatLenh_Symbol"
                            android:background="@color/lc_separation_color" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" >

                        <TextView
                            android:id="@+id/lbl_DatLenh_SoLuong"
                            style="@style/Content_Style"
                            android:layout_height="45dp"
                            android:layout_alignBottom="@+id/cus_edt_DatLenh_SoLuong"
                            android:gravity="center_vertical"
                            android:text="@string/SoLuong_2" />

                        <mobiletrading.design.Edittext_SoLuong
                            android:id="@+id/cus_edt_DatLenh_SoLuong"
                            android:layout_width="match_parent"
                            android:layout_height="45dp"
                            android:layout_marginLeft="5dp"
                            android:layout_toRightOf="@+id/lbl_DatLenh_SoLuong" />

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="0.5dp"
                            android:layout_alignLeft="@+id/lbl_DatLenh_SoLuong"
                            android:layout_alignRight="@+id/cus_edt_DatLenh_SoLuong"
                            android:layout_below="@+id/cus_edt_DatLenh_SoLuong"
                            android:background="@color/lc_separation_color" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/lay_order_pricetype"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" >

                        <TextView
                            android:id="@+id/lbl_DatLenh_LoaiLenh"
                            style="@style/Content_Style"
                            android:layout_height="45dp"
                            android:layout_alignBottom="@+id/cus_edt_DatLenh_LoaiLenh"
                            android:layout_alignParentLeft="true"
                            android:gravity="center_vertical"
                            android:text="@string/LoaiLenh" />

                        <mobiletrading.design.Edittext_LoaiLenh
                            android:id="@+id/cus_edt_DatLenh_LoaiLenh"
                            android:layout_width="match_parent"
                            android:layout_height="45dp"
                            android:layout_marginLeft="5dp"
                            android:layout_toRightOf="@+id/lbl_DatLenh_LoaiLenh" />

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="0.5dp"
                            android:layout_alignLeft="@+id/lbl_DatLenh_LoaiLenh"
                            android:layout_alignRight="@+id/cus_edt_DatLenh_LoaiLenh"
                            android:layout_below="@+id/cus_edt_DatLenh_LoaiLenh"
                            android:background="@color/lc_separation_color" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" >

                        <TextView
                            android:id="@+id/lbl_DatLenh_Gia"
                            style="@style/Content_Style"
                            android:layout_height="45dp"
                            android:layout_alignBottom="@+id/cus_edt_DatLenh_Gia"
                            android:gravity="center_vertical"
                            android:text="@string/Gia1000" />

                        <mobiletrading.design.Edittext_Gia
                            android:id="@+id/cus_edt_DatLenh_Gia"
                            android:layout_width="match_parent"
                            android:layout_height="45dp"
                            android:layout_marginLeft="5dp"
                            android:layout_toRightOf="@+id/lbl_DatLenh_Gia" />

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="0.5dp"
                            android:layout_alignLeft="@+id/lbl_DatLenh_Gia"
                            android:layout_alignRight="@+id/cus_edt_DatLenh_Gia"
                            android:layout_below="@+id/cus_edt_DatLenh_Gia"
                            android:background="@color/lc_separation_color" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/lay_order_splitqtty"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" >

                        <TextView
                            android:id="@+id/lbl_DatLenh_SplitQtty"
                            style="@style/Content_Style"
                            android:layout_height="45dp"
                            android:layout_alignBottom="@+id/edttg_DatLenh_SplitQtty"
                            android:gravity="center_vertical"
                            android:text="@string/datlenh_label_SplitQtty" />

                        <mobiletrading.design.Edittext_SoLuong
                            android:id="@+id/edttg_DatLenh_SplitQtty"
                            android:layout_width="match_parent"
                            android:layout_height="45dp"
                            android:layout_marginLeft="5dp"
                            android:layout_toRightOf="@+id/lbl_DatLenh_SplitQtty" />

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="0.5dp"
                            android:layout_alignLeft="@+id/lbl_DatLenh_SplitQtty"
                            android:layout_alignRight="@+id/edttg_DatLenh_SplitQtty"
                            android:layout_below="@+id/edttg_DatLenh_SplitQtty"
                            android:background="@color/lc_separation_color" />
                    </RelativeLayout>

                    <RelativeLayout
                    android:id="@+id/lay_order_date"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/lay_order_splitqtty"
                    android:visibility="gone" >

                    <TextView
                        android:id="@+id/lbl_order_fromdate"
                        style="@style/Label_Style"
                        android:layout_height="45dp"
                        android:layout_alignBottom="@+id/edt_order_fromdate"
                        android:layout_alignParentLeft="true"
                        android:gravity="center_vertical"
                        android:text="@string/TuNgay" />

                    <EditText
                        android:id="@+id/edt_order_fromdate"
                        style="@style/EditText_Style"
                        android:layout_width="200dp"
                        android:layout_height="45dp"
                        android:layout_marginLeft="5dp"
                        android:layout_toRightOf="@+id/lbl_order_fromdate"
                        android:gravity="center" />

                    <ImageView
                        android:layout_width="10dp"
                        android:layout_height="45dp"
                        android:layout_alignRight="@+id/edt_order_fromdate"
                        android:layout_alignTop="@+id/edt_order_fromdate"
                        android:background="@color/transparent"
                        android:clickable="false"
                        android:scaleType="fitCenter"
                        android:src="@drawable/ic_arrow" />

                    <View
                        android:id="@+id/line_order_7"
                        android:layout_width="match_parent"
                        android:layout_height="0.5dp"
                        android:layout_below="@+id/edt_order_fromdate"
                        android:background="@color/header_separation_color" />

                    <TextView
                        android:id="@+id/lbl_order_todate"
                        style="@style/Label_Style"
                        android:layout_height="45dp"
                        android:layout_alignBottom="@+id/edt_order_todate"
                        android:layout_alignParentLeft="true"
                        android:gravity="center_vertical"
                        android:text="@string/DenNgay" />

                    <EditText
                        android:id="@+id/edt_order_todate"
                        style="@style/EditText_Style"
                        android:layout_width="200dp"
                        android:layout_height="45dp"
                        android:layout_below="@+id/line_order_7"
                        android:layout_marginLeft="5dp"
                        android:layout_toRightOf="@+id/lbl_order_todate"
                        android:gravity="center" />

                    <ImageView
                        android:layout_width="10dp"
                        android:layout_height="45dp"
                        android:layout_alignRight="@+id/edt_order_todate"
                        android:layout_alignTop="@+id/edt_order_todate"
                        android:layout_centerVertical="true"
                        android:background="@color/transparent"
                        android:clickable="false"
                        android:scaleType="fitCenter"
                        android:src="@drawable/ic_arrow" />

                    <View
                        android:id="@+id/line_order_8"
                        android:layout_width="match_parent"
                        android:layout_height="0.5dp"
                        android:layout_below="@+id/edt_order_todate"
                        android:background="@color/header_separation_color" />
                </RelativeLayout>

                    <customwidget.checkbox.Checkbox
                        android:id="@+id/checkbox_placeorder_isplacegtcorder"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:checkboxActive="false"
                        app:checkboxActiveImg="@drawable/ic_checkbox_checked"
                        app:checkboxInActiveImg="@drawable/ic_checkbox_unchecked"
                        app:checkboxLabel="@string/LenhDieuKien" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="5"
                    android:orientation="vertical"
                    android:paddingBottom="10dp"
                    android:paddingLeft="@dimen/m_paddingLeft" >

                    <LinearLayout
                        android:id="@+id/lay_stockinfor_index"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal" >

                        <RelativeLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="5"
                            android:visibility="gone" >

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignBaseline="@+id/text_DatLenh_Tran"
                                android:layout_alignParentLeft="true"
                                android:text="@string/Tran"
                                android:textColor="#ff777777"
                                android:textSize="@dimen/placeorder_orderinfo_textsize" />

                            <TextView
                                android:id="@+id/text_DatLenh_Tran"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:textColor="@color/color_Tran"
                                android:textSize="@dimen/placeorder_orderinfo_textsize" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignBaseline="@+id/text_DatLenh_refprice"
                                android:layout_alignParentLeft="true"
                                android:text="@string/ThamChieu"
                                android:textColor="#ff777777"
                                android:textSize="@dimen/placeorder_orderinfo_textsize" />

                            <TextView
                                android:id="@+id/text_DatLenh_refprice"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:layout_below="@+id/text_DatLenh_Tran"
                                android:layout_marginTop="5dp"
                                android:textColor="@color/color_ThamChieu"
                                android:textSize="@dimen/placeorder_orderinfo_textsize" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignBaseline="@+id/text_DatLenh_San"
                                android:layout_alignParentLeft="true"
                                android:text="@string/datlenh_label_San"
                                android:textColor="#ff777777"
                                android:textSize="@dimen/placeorder_orderinfo_textsize" />

                            <TextView
                                android:id="@+id/text_DatLenh_San"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:layout_below="@+id/text_DatLenh_refprice"
                                android:layout_marginTop="5dp"
                                android:textColor="@color/color_San"
                                android:textSize="@dimen/placeorder_orderinfo_textsize" />

                            <TextView
                                android:id="@+id/lbl_datlenh_giakhopcuoi"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignBaseline="@+id/text_DatLenh_GiaKhopCuoi"
                                android:layout_alignParentLeft="true"
                                android:text="@string/GiaKhopCuoi"
                                android:textColor="#ff777777"
                                android:textSize="@dimen/placeorder_orderinfo_textsize" />

                            <TextView
                                android:id="@+id/text_DatLenh_GiaKhopCuoi"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:layout_below="@+id/text_DatLenh_San"
                                android:layout_marginTop="5dp"
                                android:textColor="@color/text_color"
                                android:textSize="@dimen/placeorder_orderinfo_textsize" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="2.5" >

                            <TextView
                                android:id="@+id/lbl_datlenh_SucMuaBuy"
                                android:layout_width="wrap_content"
                                android:layout_height="@dimen/placeorder_sucmua_tylevay_height"
                                android:paddingLeft="5dp"
                                android:text="@string/datlenh_label_SucMua"
                                android:textColor="@color/text_color"
                                android:textSize="@dimen/placeorder_orderinfo_textsize" />

                            <TextView
                                android:id="@+id/text_datlenh_SucMuaBuy"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/placeorder_sucmua_tylevay_height"
                                android:layout_marginLeft="5dp"
                                android:layout_toRightOf="@+id/lbl_datlenh_SucMuaBuy"
                                android:gravity="right"
                                android:textColor="@color/text_color"
                                android:textSize="@dimen/placeorder_orderinfo_textsize" />

                            <TextView
                                android:id="@+id/lbl_datlenh_rttbuy"
                                android:layout_width="wrap_content"
                                android:layout_height="@dimen/placeorder_sucmua_tylevay_height"
                                android:layout_below="@+id/lbl_datlenh_SucMuaBuy"
                                android:paddingLeft="5dp"
                                android:text="@string/RTT"
                                android:textColor="@color/text_color"
                                android:textSize="@dimen/placeorder_orderinfo_textsize" />

                            <TextView
                                android:id="@+id/text_datlenh_rttbuy"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/placeorder_sucmua_tylevay_height"
                                android:layout_below="@+id/lbl_datlenh_SucMuaBuy"
                                android:layout_marginLeft="5dp"
                                android:layout_toRightOf="@+id/lbl_datlenh_rttbuy"
                                android:gravity="right"
                                android:textColor="@color/text_color"
                                android:textSize="@dimen/placeorder_orderinfo_textsize" />

                            <TextView
                                android:id="@+id/lbl_datlenh_SucMuaSell"
                                android:layout_width="wrap_content"
                                android:layout_height="@dimen/placeorder_sucmua_tylevay_height"
                                android:layout_alignBottom="@+id/text_datlenh_SucMuaSell"
                                android:paddingLeft="5dp"
                                android:text="@string/datlenh_label_SucMua"
                                android:textColor="@color/text_color"
                                android:textSize="@dimen/placeorder_orderinfo_textsize" />

                            <TextView
                                android:id="@+id/text_datlenh_SucMuaSell"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/placeorder_sucmua_tylevay_height"
                                android:layout_below="@+id/text_datlenh_rttbuy"
                                android:layout_marginLeft="5dp"
                                android:layout_toRightOf="@+id/lbl_datlenh_SucMuaSell"
                                android:gravity="right"
                                android:textColor="@color/text_color"
                                android:textSize="@dimen/placeorder_orderinfo_textsize" />

                            <TextView
                                android:id="@+id/lbl_datlenh_rttsell"
                                android:layout_width="wrap_content"
                                android:layout_height="@dimen/placeorder_sucmua_tylevay_height"
                                android:layout_alignBottom="@+id/text_datlenh_rttsell"
                                android:paddingLeft="5dp"
                                android:text="@string/RTT"
                                android:textColor="@color/text_color"
                                android:textSize="@dimen/placeorder_orderinfo_textsize" />

                            <TextView
                                android:id="@+id/text_datlenh_rttsell"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/placeorder_sucmua_tylevay_height"
                                android:layout_below="@+id/text_datlenh_SucMuaSell"
                                android:layout_marginLeft="5dp"
                                android:layout_toRightOf="@+id/lbl_datlenh_rttsell"
                                android:gravity="right"
                                android:textColor="@color/text_color"
                                android:textSize="@dimen/placeorder_orderinfo_textsize" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="2.5"
                            android:paddingLeft="10dp" >

                            <TextView
                                android:id="@+id/lbl_datlenh_tylevay"
                                android:layout_width="wrap_content"
                                android:layout_height="@dimen/placeorder_sucmua_tylevay_height"
                                android:layout_alignBottom="@+id/text_datlenh_tylevay"
                                android:text="@string/datlenh_label_TyLeVay"
                                android:textColor="@color/text_color"
                                android:textSize="@dimen/placeorder_orderinfo_textsize" />

                            <TextView
                                android:id="@+id/text_datlenh_tylevay"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/placeorder_sucmua_tylevay_height"
                                android:layout_marginLeft="5dp"
                                android:layout_toRightOf="@+id/lbl_datlenh_tylevay"
                                android:gravity="right"
                                android:textColor="@color/text_color"
                                android:textSize="@dimen/placeorder_orderinfo_textsize" />

                            <TextView
                                android:id="@+id/lbl_datlenh_KLduocmua"
                                android:layout_width="wrap_content"
                                android:layout_height="@dimen/placeorder_sucmua_tylevay_height"
                                android:layout_below="@+id/lbl_datlenh_tylevay"
                                android:text="@string/datlenh_label_KLDuocMua"
                                android:textColor="@color/text_color"
                                android:textSize="@dimen/placeorder_orderinfo_textsize" />

                            <TextView
                                android:id="@+id/text_datlenh_KLduocmua"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/placeorder_sucmua_tylevay_height"
                                android:layout_below="@+id/lbl_datlenh_tylevay"
                                android:layout_marginLeft="5dp"
                                android:layout_toRightOf="@+id/lbl_datlenh_KLduocmua"
                                android:gravity="right"
                                android:textColor="@color/text_color"
                                android:textSize="@dimen/placeorder_orderinfo_textsize" />

                            <TextView
                                android:id="@+id/lbl_datlenh_ChoVe"
                                android:layout_width="wrap_content"
                                android:layout_height="@dimen/placeorder_sucmua_tylevay_height"
                                android:layout_alignBottom="@+id/text_datlenh_ChoVe"
                                android:text="@string/datlenh_label_ChoVe"
                                android:textColor="@color/text_color"
                                android:textSize="@dimen/placeorder_orderinfo_textsize" />

                            <TextView
                                android:id="@+id/text_datlenh_ChoVe"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/placeorder_sucmua_tylevay_height"
                                android:layout_below="@+id/text_datlenh_KLduocmua"
                                android:layout_marginLeft="5dp"
                                android:layout_toRightOf="@+id/lbl_datlenh_ChoVe"
                                android:gravity="right"
                                android:textColor="@color/text_color"
                                android:textSize="@dimen/placeorder_orderinfo_textsize" />

                            <TextView
                                android:id="@+id/lbl_datlenh_KLduocban"
                                android:layout_width="wrap_content"
                                android:layout_height="@dimen/placeorder_sucmua_tylevay_height"
                                android:layout_alignBottom="@+id/text_datlenh_KLduocban"
                                android:text="@string/datlenh_label_KLduocban"
                                android:textColor="@color/text_color"
                                android:textSize="@dimen/placeorder_orderinfo_textsize" />

                            <TextView
                                android:id="@+id/text_datlenh_KLduocban"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/placeorder_sucmua_tylevay_height"
