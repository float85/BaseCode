<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/bg_fragment"
    android:orientation="vertical"
    android:paddingLeft="@dimen/m_paddingLeft" >

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TextView
            android:id="@+id/text_resultcancelallorder_item_custodycd"
            style="@style/Content_Style"
            android:layout_width="wrap_content" />

        <TextView
            android:id="@+id/text_resultcancelallorder_item_side"
            style="@style/Content_Style"
            android:layout_width="wrap_content"
            android:layout_below="@+id/text_resultcancelallorder_item_custodycd" />

        <TextView
            android:id="@+id/text_resultcancelallorder_item_symbol"
            style="@style/Content_Style"
            android:layout_width="wrap_content"
            android:layout_below="@+id/text_resultcancelallorder_item_custodycd"
            android:layout_marginLeft="50dp"
            android:layout_toRightOf="@+id/text_resultcancelallorder_item_side" />

        <ImageView
            android:id="@+id/img_resultcancelallorder_item_issuccess"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dp"
            android:scaleType="fitCenter"
            android:src="@drawable/selector_docancelorer_issuccess" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="@color/listview_separation_color" />

</LinearLayout>