<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res/com.fss.tcbs.mobiletrading"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/bg_fragment"
    android:orientation="vertical" >

    <mobiletrading.design.LabelContentLayout
        android:id="@+id/text_cashtransferitemdetail_beneficiaryname"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:labelText="@string/HoTen"
        app:orientation="horizontal" />

    <mobiletrading.design.LabelContentLayout
        android:id="@+id/text_cashtransferitemdetail_bankaccount"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:labelText="@string/TaiKhoan"
        app:orientation="horizontal" />

    <mobiletrading.design.LabelContentLayout
        android:id="@+id/text_cashtransferitemdetail_bank"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:labelText="@string/NganHang"
        app:orientation="horizontal"
        app:singleLine="true" />

    <mobiletrading.design.LabelContentLayout
        android:id="@+id/text_cashtransferitemdetail_branch"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:labelText="@string/PhongGiaoDich"
        app:orientation="horizontal" />

    <mobiletrading.design.LabelContentLayout
        android:id="@+id/text_cashtransferitemdetail_city"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:labelText="@string/TinhThanhPho"
        app:orientation="horizontal" />

    <mobiletrading.design.LabelContentLayout
        android:id="@+id/text_cashtransferitemdetail_custodycd"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:labelText="@string/SoTaiKhoanNhan"
        app:orientation="horizontal" />

    <mobiletrading.design.LabelContentLayout
        android:id="@+id/text_cashtransferitemdetail_afacctno"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:labelText="@string/TieuKhoanNhan"
        app:orientation="horizontal" />

</LinearLayout>