<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@color/bg_fragment"
    android:padding="5dp" >

    <TextView
        android:id="@+id/lbl_alert_DotBienGia_KL_MaCK"
        android:layout_width="55dp"
        android:layout_height="30dp"
        android:layout_marginTop="5dp"
        android:gravity="center_vertical"
        android:text="@string/MaCK"
        android:textColor="#ffffffff"
        android:textSize="10.0sp" />

    <EditText android:textCursorDrawable="@drawable/shape_textcursoredittext"
        android:id="@+id/edt_alert_DotBienGia_KL_MaCK"
        style="@style/EditText_Style"
        android:layout_width="fill_parent"
        android:layout_marginLeft="5dp"
        android:layout_marginTop="5dp"
        android:layout_toRightOf="@+id/lbl_alert_DotBienGia_KL_MaCK"
        android:hint="@string/CK1CK2CK3"
        android:inputType="textCapCharacters"
        android:maxLines="4"
        android:singleLine="true"
        android:textSize="15.0sp" />

    <TextView
        android:id="@+id/lbl_alert_DotBienGia_KL_GiaDotBien"
        android:layout_width="55dp"
        android:layout_height="30dp"
        android:layout_below="@+id/lbl_alert_DotBienGia_KL_MaCK"
        android:layout_marginTop="5dp"
        android:gravity="bottom"
        android:text="@string/dotbiengia_kl_lbl_GiaDotBien"
        android:textColor="#ffffffff"
        android:textSize="10.0sp" />

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="30dp"
        android:layout_below="@+id/lbl_alert_DotBienGia_KL_MaCK"
        android:layout_marginLeft="5dp"
        android:layout_marginTop="5dp"
        android:layout_toRightOf="@+id/lbl_alert_DotBienGia_KL_GiaDotBien"
        android:weightSum="12.0" >

        <Button
            android:id="@+id/btn_alert_DotBienGia_KL_Tang1"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="2.0"
            android:background="@drawable/button_selected2"
            android:textColor="#ffffffff"
            android:textSize="10.0sp" />

        <Button
            android:id="@+id/btn_alert_DotBienGia_KL_Tang2"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="2.0"
            android:background="@drawable/button_not_select2"
            android:textColor="#ffffffff"
            android:textSize="10.0sp" />

        <Button
            android:id="@+id/btn_alert_DotBienGia_KL_Tang3"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="2.0"
            android:background="@drawable/button_not_select2"
            android:textColor="#ffffffff"
            android:textSize="10.0sp" />

        <Button
            android:id="@+id/btn_alert_DotBienGia_KL_Giam1"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="2.0"
            android:background="@drawable/button_not_select2"
            android:textColor="#ffffffff"
            android:textSize="10.0sp" />

        <Button
            android:id="@+id/btn_alert_DotBienGia_KL_Giam2"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="2.0"
            android:background="@drawable/button_not_select2"
            android:textColor="#ffffffff"
            android:textSize="10.0sp" />

        <Button
            android:id="@+id/btn_alert_DotBienGia_KL_Giam3"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="2.0"
            android:background="@drawable/button_not_select2"
            android:textColor="#ffffffff"
            android:textSize="10.0sp" />
    </LinearLayout>

    <TextView
        android:id="@+id/lbl_alert_DotBienGia_KL_KhoiLuongDB"
        android:layout_width="55dp"
        android:layout_height="30dp"
        android:layout_below="@+id/lbl_alert_DotBienGia_KL_GiaDotBien"
        android:layout_marginTop="5dp"
        android:gravity="bottom"
        android:text="@string/dotbienkhoiluong_lbl_KhoiLuongDB"
        android:textColor="#ffffffff"
        android:textSize="10.0sp" />

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="30dp"
        android:layout_below="@+id/lbl_alert_DotBienGia_KL_GiaDotBien"
        android:layout_marginLeft="5dp"
        android:layout_marginTop="5dp"
        android:layout_toRightOf="@+id/lbl_alert_DotBienGia_KL_KhoiLuongDB" >

        <Button
            android:id="@+id/btn_alert_DotBienGia_KL_100"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="2.0"
            android:background="@drawable/button_selected2"
            android:textColor="#ffffffff"
            android:textSize="10.0sp" />

        <Button
            android:id="@+id/btn_alert_DotBienGia_KL_200"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="2.0"
            android:background="@drawable/button_not_select2"
            android:textColor="#ffffffff"
            android:textSize="10.0sp" />

        <Button
            android:id="@+id/btn_alert_DotBienGia_KL_300"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="2.0"
            android:background="@drawable/button_not_select2"
            android:textColor="#ffffffff"
            android:textSize="10.0sp" />

        <Button
            android:id="@+id/btn_alert_DotBienGia_KL_400"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="2.0"
            android:background="@drawable/button_not_select2"
            android:textColor="#ffffffff"
            android:textSize="10.0sp" />

        <Button
            android:id="@+id/btn_alert_DotBienGia_KL_500"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="2.0"
            android:background="@drawable/button_not_select2"
            android:textColor="#ffffffff"
            android:textSize="10.0sp" />
    </LinearLayout>

    <TextView
        android:id="@+id/lbl_alert_DotBienGia_KL_DotBienTrong"
        android:layout_width="55dp"
        android:layout_height="30dp"
        android:layout_below="@+id/lbl_alert_DotBienGia_KL_KhoiLuongDB"
        android:layout_marginTop="5dp"
        android:gravity="bottom"
        android:text="@string/dotbienkhoiluong_lbl_DotBienTrong"
        android:textColor="#ffffffff"
        android:textSize="10.0sp" />

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="30dp"
        android:layout_below="@+id/lbl_alert_DotBienGia_KL_KhoiLuongDB"
        android:layout_marginLeft="5dp"
        android:layout_marginTop="5dp"
        android:layout_toRightOf="@+id/lbl_alert_DotBienGia_KL_DotBienTrong" >

        <Button
            android:id="@+id/btn_alert_DotBienGia_KL_1thang"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="2.0"
            android:background="@drawable/button_selected2"
            android:textColor="#ffffffff"
            android:textSize="10.0sp" />

        <Button
            android:id="@+id/btn_alert_DotBienGia_KL_2thang"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="2.0"
            android:background="@drawable/button_not_select2"
            android:textColor="#ffffffff"
            android:textSize="10.0sp" />

        <Button
            android:id="@+id/btn_alert_DotBienGia_KL_3thang"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="2.0"
            android:background="@drawable/button_not_select2"
            android:textColor="#ffffffff"
            android:textSize="10.0sp" />

        <Button
            android:id="@+id/btn_alert_DotBienGia_KL_6thang"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="2.0"
            android:background="@drawable/button_not_select2"
            android:textColor="#ffffffff"
            android:textSize="10.0sp" />

        <Button
            android:id="@+id/btn_alert_DotBienGia_KL_12thang"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="2.0"
            android:background="@drawable/button_not_select2"
            android:textColor="#ffffffff"
            android:textSize="10.0sp" />
    </LinearLayout>

    <TextView
        android:id="@+id/lbl_alert_DotBienGia_KL_TyleDB"
        android:layout_width="55dp"
        android:layout_height="30dp"
        android:layout_below="@+id/lbl_alert_DotBienGia_KL_DotBienTrong"
        android:layout_marginTop="5dp"
        android:gravity="bottom"
        android:text="@string/dotbienkhoiluong_lbl_TyleDB"
        android:textColor="#ffffffff"
        android:textSize="10.0sp" />

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="30dp"
        android:layout_below="@+id/lbl_alert_DotBienGia_KL_DotBienTrong"
        android:layout_marginLeft="5dp"
        android:layout_marginTop="5dp"
        android:layout_toRightOf="@+id/lbl_alert_DotBienGia_KL_TyleDB" >

        <Button
            android:id="@+id/btn_alert_DotBienGia_KL_2lan"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="2.0"
            android:background="@drawable/button_selected2"
            android:textColor="#ffffffff"
            android:textSize="10.0sp" />

        <Button
            android:id="@+id/btn_alert_DotBienGia_KL_3lan"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="2.0"
            android:background="@drawable/button_not_select2"
            android:textColor="#ffffffff"
            android:textSize="10.0sp" />

        <Button
            android:id="@+id/btn_alert_DotBienGia_KL_4lan"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="2.0"
            android:background="@drawable/button_not_select2"
            android:textColor="#ffffffff"
            android:textSize="10.0sp" />

        <Button
            android:id="@+id/btn_alert_DotBienGia_KL_5lan"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="2.0"
            android:background="@drawable/button_not_select2"
            android:textColor="#ffffffff"
            android:textSize="10.0sp" />

        <Button
            android:id="@+id/btn_alert_DotBienGia_KL_6lan"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="2.0"
            android:background="@drawable/button_not_select2"
            android:textColor="#ffffffff"
            android:textSize="10.0sp" />
    </LinearLayout>

    <Button
        android:id="@+id/btn_alert_DotBienGia_KL_ChapNhan"
        style="@style/Button_Background1"
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:layout_below="@+id/lbl_alert_DotBienGia_KL_TyleDB"
        android:layout_marginTop="10dp"
        android:text="@string/XacNhan" />

    <TextView
        android:id="@+id/text_alert_DotBienGia_KL_KetQuaBoLoc"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:layout_below="@+id/lbl_alert_DotBienGia_KL_TyleDB"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:layout_toRightOf="@+id/btn_alert_DotBienGia_KL_ChapNhan"
        android:gravity="center_vertical"
        android:textColor="#ffffffff"
        android:textSize="11.0sp" />

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/btn_alert_DotBienGia_KL_ChapNhan"
        android:layout_marginTop="5dp"
        android:orientation="vertical" >

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:weightSum="10.0" >

            <TextView
                android:id="@+id/lbl_alert_DotBienGia_KL_title_MaCK"
                style="@style/Label_Style"
                android:layout_width="0dp"
                android:layout_weight="2.0"
                android:text="@string/MaCK" />

            <TextView
                android:id="@+id/lbl_alert_DotBienGia_KL_GiaKhopCuoi"
                style="@style/Label_Style"
                android:layout_width="0dp"
                android:layout_weight="4.0"
                android:text="@string/GiaKhopCuoi" />

            <TextView
                android:id="@+id/lbl_alert_DotBienGia_KL_TongKLGD"
                style="@style/Label_Style"
                android:layout_width="0dp"
                android:layout_weight="4.0"
                android:text="@string/dotbienkhoiluong_lbl_TongKLGD" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="1dp"
            android:background="#ffff9900" />

        <ListView
            android:id="@+id/listview_alert_DotBienGia_KL"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:divider="@color/listview_separation_color" android:scrollbars="none" />
    </LinearLayout>

</RelativeLayout>